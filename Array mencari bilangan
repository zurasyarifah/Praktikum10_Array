#include <iostream>
using namespace std;

void main()
{
	int arr[5], cari, toogle=0;
		
	for (int i=0;i<5;i++)
	{
		cout<<"masukkan bilangan ke "<<i+1<<" = ";
		cin>>arr[i];
		cout<<endl;
	}
	cout<<endl;

	cout<<"cari bilangan: ";
	cin>>cari;
	cout<<endl;

	for (int i=0;i<5;i++)
	{
		if (cari==arr[i])
		{
			cout<<"bilangan ditemukan di "<<i+1<<endl;
			toogle=1;
		}

	}
	if (toogle==0)
		{
			cout<<"bilangan tidak ditemukan"<<endl;
		}

}
